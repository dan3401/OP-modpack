@dyson_scale = 40
@spyorb_scale = 6

entity = {
	name = "eternal_empire_gateway_entity"
	pdxmesh = "gateway_mesh"
	
	scale = 1.5
	locator = { name = "root"	position = { 0 0 0 } rotation = { 0 0 0 } }		
	locator = { name = "attacher"	position = { 0 0 0 } rotation = { 0 0 -90 } }	
	locator = { name = "attacher3"	position = { 0 0 0 } rotation = { 90 0 0 } }	

	
	default_state = "idle"
	state = { name = "idle" animation = "idle"
		start_event = { trigger_once = yes sound = { soundeffect = "megastructure_gate_way_station_hum" stop_on_state_change = yes } }
		start_event = { trigger_once = yes sound = { soundeffect = "megastructure_gate_way_portal_hum" stop_on_state_change = yes } }
		
		event = { time = 0 node = "light_locator_01" particle = "ship_large_light_eternal_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "light_locator_02" particle = "ship_large_light_eternal_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "light_locator_03" particle = "ship_large_light_eternal_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "light_locator_04" particle = "ship_large_light_eternal_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "light_locator_05" particle = "ship_large_light_eternal_effect" keep_particle = yes trigger_once = yes }
		event = { time = 0 node = "light_locator_06" particle = "ship_large_light_eternal_effect" keep_particle = yes trigger_once = yes }
	}
	state = { name = "death" animation = "death" looping = no
		event = { time = 7.63 node = "root" particle = "dreadnought_explosion_particle" keep_particle = yes }
		event = { time = 2.1 node = "root" particle = "gateway_portal_death_residue_effect" keep_particle = yes }
		
		event = { trigger_once = yes sound = { soundeffect = "megastructure_gate_way_station_death" stop_on_state_change = yes } }
	}
	
		meshsettings = {
			name = "gateway_bottomShapeShape"
			index = 0
			texture_diffuse = "eternal_empire_gateway_diffuse.dds"
			texture_normal = "eternal_empire_gateway_normal.dds"
			texture_specular = "eternal_empire_gateway_specular.dds"
			shader = "PdxMeshShip"
		}
		
	locator = { name = "slot1"				position = { 0	0  0 } rotation = { 0 45 0 } }
	
	
	attach = { portal_joint = "phi_portal_enity" }
	attach = { root = "gateway_portal_entity" }
	#attach = { root = "eternal_empire_planet_shield_effect_entity" }
	attach = { root = "eternal_empire_gateway_attachment_entity" }
	#attach = { attacher3 = "eternal_empire_gateway_attachment_entity" }
}

